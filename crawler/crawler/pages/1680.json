         b'stitution_Lemma_2";\n\n\nGoal "(A Un B) Int C = {} ==&gt; A Int C ='
         b' {} &amp; B Int C = {}";\nby(Blast_tac 1);\nqed "disjoint_monotini'
         b'city";\n\n\nGoal "[|x:FV(e\'); x~=y|] ==&gt; (Capt y e) Int FV(e'
         b"') = {} --&gt; y:FV(e) --&gt; Capt x (e[y:=e']) = Capt x e Un Capt x"
         b' e\' Un Capt y e";\nby(induct_tac "e" 1);\nby(ALLGOALS strip_tac);\n'
         b'by(Asm_full_simp_tac 1);\nby(Asm_full_simp_tac 1);\nby(dtac disjoi'
         b'nt_monotinicity 1);\nby(Asm_full_simp_tac 1);\nby(etac disjE 1);\nb'
         b'y(case_tac "y:FV(lterm2)" 1);\nby(Asm_full_simp_tac 1);\nby(Blast_'
         b'tac 1);\nby(ftac (Capt_FV_lemma_1 RS mp) 1);\nby(Asm_full_simp_tac'
         b' 1);\nby(Blast_tac 1);\nby(case_tac "y:FV(lterm1)" 1);\nby(Asm_full'
         b'_simp_tac 1);\nby(Blast_tac 1);\nby(ftac (Capt_FV_lemma_1 RS mp) 1'
         b');\nby(Asm_full_simp_tac 1);\nby(Blast_tac 1);\nby(asm_full_simp_ta'
         b'c (simpset() delsimps[Capt_Abs]) 1);\nby(etac conjE 1);\nby(fatac '
         b'Capt_Int_FV_lemma_1 1 1);\nby(rotate_tac ~2 1);\nby(ftac not_sym 1'
         b');\nby(rotate_tac ~1 1);\nby(fatac Capt_induct_3 1 1);\nby(mp_tac 1'
         b');\nby(rtac conjI 1);\nby(rtac conjI 2);\nby(ALLGOALS strip_tac);\nb'
         b'y(contr_tac 2);\nby(Fast_tac 2);\nby(etac conjE 1);\nby(Asm_simp_ta'
         b'c 1);\nby(rtac conjI 1);\nby(ALLGOALS strip_tac);\nby(rtac conjI 2)'
         b';\nby(ALLGOALS strip_tac);\nby(rotate_tac ~2 2);\nby(dtac (Capt_FV_'
