         b'y(case_tac "x=var" 1);\nby(Asm_full_simp_tac 1);\nby(etac conjI 1)'
         b';\nby(dtac FV_subst_lemma_6 1);\nby(Fast_tac 1);\nqed "Capt_subst_d'
         b'istribute";\n\n\n(* Substitution Lemma - Formulation 3 *)\n\nGoal'
         b' "[|Capt x e Int A = {}; y~:FV(e); y~:BV(e); x~=y|] ==&gt; Capt y (e'
         b'[x:=Var y]) Int A = {}";\nby(case_tac "x:FV(e)" 1);\nby(dtac (Capt'
         b'_FV_lemma_1 RS mp) 2);\nby(Asm_simp_tac 2);\nby(stac (Capt_subst_d'
         b'istribute RS mp RS mp) 1);\nby(Simp_tac 1);\nby(fatac not_sym 1 1)'
         b';\nby(atac 2);\nby(dtac (Capt_BV_lemma_1 RS mp) 1);\nby((Simp_tac 1'
         b') THEN (Fast_tac 1));\nby(dtac (Capt_FV_lemma_1 RS mp) 1);\nby(Asm'
         b'_simp_tac 1);\nqed "Capt_subst_var_lemma_1";\n\n\n(* Substitution Le'
         b'mma - Formulation 3 *)\n\nGoal "[|Capt x e Int FV(e\') = {}; y~:FV('
         b'e); y~:BV(e)|] ==&gt; e[x:=Var y][y:=e\'] = e[x:=e\']";\nby(case_ta'
         b'c "x=y" 1);\nby(Asm_full_simp_tac 1);\nby(stac (Substitution_Lemma'
         b'_2 RS mp RS mp RS mp RS mp RS sym) 1);\nby(ALLGOALS Asm_simp_tac)'
         b';\nby(dres_inst_tac [("x1","x")] (Capt_BV_lemma_1 RS mp) 1);\nby(F'
         b'ast_tac 1);\nby(etac Capt_subst_var_lemma_1 1);\nby(ALLGOALS atac)'
         b';\nqed "Substitution_Lemma_3";\n\n\n(* Substitution Lemma - Formulat'
         b'ion 4 *)\n\nGoal "[|y~=z; x~=z; x~:FV(e\')|] ==&gt; e[x:=Var y][z:='
         b'e\'] = e[z:=e\'][x:=Var y]";\nby(induct_tac "e" 1);\nby(Asm_simp_tac'
         b' 1);\nby(Asm_simp_tac 1);\nby(case_tac "var=x" 1);\nby(Asm_simp_tac'
         b' 1);\nby(case_tac "var=z" 1);\nby(Asm_simp_tac 1);\nby(Asm_simp_tac'
         b' 1);\nqed "Substitution_Lemma_4";</pre>\n<hr>\n\n</body>\n</html>\n',
